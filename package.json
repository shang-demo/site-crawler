{
  "name": "headless-chrome",
  "version": "0.0.1",
  "description": "run code with puppeteer",
  "main": "index.js",
  "scripts": {
    "start": "ts-node --project tsconfig.json --files --pretty --log-error index.ts",
    "build": "rm -rf ./dist/ && tsc -p tsconfig.json && cp package.json pnpm-lock.yaml ./dist && cp src/index.html src/headless-test.html ./dist/src/",
    "start:docker": "node index.js",
    "lint": "eslint --format './node_modules/eslint-formatter-friendly/index.js' \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "puppeteer",
    "headless-chrome"
  ],
  "author": "xinshangshangxin",
  "license": "MIT",
  "dependencies": {
    "@koa/router": "^12.0.0",
    "bluebird": "^3.7.2",
    "dayjs": "^1.8.24",
    "fs-extra": "^11.1.0",
    "humps": "^2.0.1",
    "koa": "^2.11.0",
    "koa-body": "^6.0.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "puppeteer": "^19.6.2",
    "socket.io": "^4.5.4",
    "uuid": "^9.0.0",
    "vm2": "^3.9.1"
  },
  "devDependencies": {
    "@s4p/eslint-config": "5.2.0",
    "@types/bluebird": "^3.5.30",
    "@types/fs-extra": "^11.0.1",
    "@types/humps": "^2.0.0",
    "@types/koa": "^2.11.3",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^12.0.0",
    "@types/lodash": "^4.14.150",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
