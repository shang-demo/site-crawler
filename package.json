{
  "name": "headless-chrome",
  "version": "0.0.1",
  "description": "run code with puppeteer",
  "main": "index.js",
  "scripts": {
    "start": "ts-node --project tsconfig.json --files --pretty --log-error index.ts",
    "build": "rm -rf ./dist/ && tsc -p tsconfig.json && cp package.json package-lock.json Dockerfile ./dist && cp src/index.html src/headless-test.html ./dist/src/",
    "start:docker": "node index.js",
    "lint": "eslint --format './node_modules/eslint-formatter-friendly/index.js' \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "puppeteer",
    "headless-chrome"
  ],
  "author": "xinshangshangxin",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.7.2",
    "dayjs": "^1.8.24",
    "humps": "^2.0.1",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-router": "^8.0.8",
    "lodash": "^4.17.15",
    "puppeteer": "^3.0.0",
    "socket.io": "^2.3.0",
    "vm2": "^3.9.1"
  },
  "devDependencies": {
    "@ofa2/eslint-config": "^3.4.0",
    "@types/bluebird": "^3.5.30",
    "@types/humps": "^2.0.0",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.0",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.11.1",
    "@types/puppeteer-core": "^2.0.0",
    "@types/socket.io": "^2.1.8",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
